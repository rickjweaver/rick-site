<!doctype html>
<html lang="en">
<head>
  <title>{% block page_title %}Rick Weaver{% endblock %}</title>
  <!-- Open Graph Meta Tags -->
  <meta property="og:site_name" content="Rick Weaver">
  <meta property="og:url" content="rickjweaver.com">
  <meta property="og:image" content="assets/img/BlueIcon.jpg">
  <meta name="description" content="{% block page_description %}Music, video, text{% endblock %}">
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <link rel="icon" href="assets/img/BlueIcon.png" type="image/png" sizes="16x16" />
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <link rel="stylesheet" href="css/main.css">
  <link href="https://fonts.googleapis.com/css?family=Khula|Tangerine|Cinzel" rel="stylesheet">
</head>
<body class="bg">
  {% include "_partials/nav.njk" %}
  {# {% include '_partials/jumbotron.njk' %} #}

  {% block content %}{% endblock %}

  {% include "_partials/footer.njk" %}

  <script src="js/jquery.js"></script>
  <script src="js/popper.js"></script>
  <script src="js/bootstrap.js"></script>
  <script src="js/scripts.js"></script>
  <script src="js/nav.js"></script>

</body>
</html>
